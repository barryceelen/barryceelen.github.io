/**
 * Variables defined here because we need the pem mixin
 */
.hidden {
  display: none;
}

img {
  display: block;
}

figure {
  margin: 0;
  padding: 0;
}

picture {
  display: block;
  margin: 0;
  padding: 0;
}

h1, p {
  margin: 0;
  padding: 0;
}

html, body {
  margin: 0;
  padding: 0;
}

body {
  background: white;
  color: #222222;
  font-family: "Roboto Condensed", sans-serif;
  font-size: 100%;
}

#content {
  background: white;
}

.site-footer {
  margin: 0 auto;
  max-width: 1100px;
  padding: 6em 2em 2em;
}

.site-title {
  font-size: 1em;
  font-weight: 400;
}
.site-title a {
  color: #222222;
  text-decoration: none;
  text-transform: uppercase;
}

.site-branding h1 {
  margin: 0;
}

.project {
  border-bottom: 1px solid #e6e6e6;
  margin: 0;
  overflow-x: hidden;
  text-rendering: optimizeLegibility;
  font-feature-settings: "kern";
  -webkit-font-feature-settings: "kern";
  -moz-font-feature-settings: "kern";
  -moz-font-feature-settings: "kern=1";
}

.project-inner {
  margin: 0 auto;
  max-width: 1100px;
}

.project-iphone {
  padding: 2em;
}
.project-iphone .project-details {
  float: left;
  width: 340px;
}
.project-iphone .project-gallery {
  float: right;
  width: 760px;
}

.project-web {
  padding: 2em 2em 0;
}

.project-details {
  margin-bottom: 2em;
  padding-right: 1em;
}
.project-details .entry-title {
  font-size: 2.75em;
  font-weight: bold;
  line-height: 1;
  margin-bottom: 0.13636em;
  text-transform: uppercase;
}
.project-details .entry-content {
  font-size: 1.125em;
  line-height: 1;
  text-transform: uppercase;
  word-spacing: .1em;
}

.project-gallery {
  white-space: nowrap;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  -o-user-select: none;
  user-select: none;
}
.project-gallery figure {
  background: white;
  margin-right: 1.5em;
}
.project-gallery figure:last-of-type {
  margin-right: 0;
}

